<% if(productos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Nombre</th> <th>Precio</th> <th>Imagen</th> </tr>
            <% productos.forEach(function(producto) { %>
            <tr>
                <td><%= producto.title %></td>
                <td><%= producto.price %></td>
                <td><img src="<%= producto.thumbnail %>" alt="<%= producto.title %>" title="<%= producto.title %>" height="100" width="100"/></td>
            </tr>
            <% }); %>
        </table>
    </div>
<% } else{ %>
    <h3 class="alert alert-danger">No se encontraron productos!</h1>
<% } %>
